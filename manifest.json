{
  "name": "Pin Github",
  "description": "Pin subfields for your PR & Issues",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "src/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*/issues/new"],
      "js": ["dist/contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
